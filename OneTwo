/*12,123,1234,12345 upto user input iteration.
Use a loop of your choice.*/
import java.util.Scanner;
public class OneTwo
{
  public static void main(String args[])
  {
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter the maximum iteration point")
    int term = sc.nextInt(), product=1, increment;
    for(increment=2; increment<=term; increment++)
    {
      product = (product*10)+ increment;
      System.out.print(product);
    }
  }
}
